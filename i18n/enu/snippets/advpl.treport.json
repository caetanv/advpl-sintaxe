      "Return oReport",
      "",
      "",
      "/*/{Protheus.doc} ReportPrint",
      "\tMontar query e abrir em tabela temporária",
      "",
      "\t@author  ${Name}",
      "\t@example ${Example}",
      "\t@param   [${Parameter_Name}],${Parameter_type},${Parameter_Description}",
      "\t@return  ${Return}",
      "\t@table   ${Tables}",
      "\t@since   ${CURRENT_DATE}-${CURRENT_MONTH}-${CURRENT_YEAR}",
      "/*/",
      "",
      "static function ReportPrint(oReport, ccAlias,cOrdem)",
      "\tlocal oSection1b := oReport:Section(1)",
      "\tlocal oSection2b := ''",
      "\tlocal oSection3b := ''",
      "\tlocal oSection4b := ''",
      "\tlocal oSection5b := ''",
      "",
      "\toSection2b := oReport:Section(1):Section(1)",
      "\toSection3b := oReport:Section(1):Section(1):Section(1)",
      "\toSection4b := oReport:Section(1):Section(1):Section(1):Section(1)",
      "\toSection5b := oReport:Section(1):Section(1):Section(1):Section(1):Section(1)",
      "",
      "\toSection1b:BeginQuery()",
      "\tBeginSQL Alias ccAlias",
      "\t    %noparser%",
      "",
      "\t    ${query}",
      "",
      "\tEndSql",
      "\toSection1b:EndQuery()",
      "",
      "\toSection2b:SetParentQuery()",
      "\toSection3b:SetParentQuery()",
      "\toSection4b:SetParentQuery()",
      "\toSection5b:SetParentQuery()",
      "",
      "\toReport:SetMeter((ccAlias)->(RecCount()))",
      "",
      "\toSection2b:SetParentFilter({|cParam| (ccAlias)->${cellSession1} == cParam}, {|| (ccAlias)->${cellSession1}})",
      "",
      "\toSection3b:SetParentFilter({|cParam| (ccAlias)->(${cellSession1} + ${cellSession2})  == cParam}, {|| (ccAlias)->(${cellSession1} + ${cellSession2})})",
      "",
      "\toSection4b:SetParentFilter({|cParam| (ccAlias)->(${cellSession1} + ${cellSession2} + ${cellSession3})  == cParam}, {|| (ccAlias)->(${cellSession1} + ${cellSession2} + ${cellSession3})})",
      "",
      "\toSection5b:SetParentFilter({|cParam| (ccAlias)->(${cellSession1} + ${cellSession2} + ${cellSession3} + ${cellSession4})  == cParam}, {|| (ccAlias)->(${cellSession1} + ${cellSession2} + ${cellSession3} + ${cellSession4})})",
      "",
      "\toSection1b:Print()",
      "return",
      "",
      "/*/{Protheus.doc} xCriaPerg",
