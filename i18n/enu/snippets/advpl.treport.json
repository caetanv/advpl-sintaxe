      "\t@return  ${Return}",
      "\t@table   ${Tables}",
      "\t@since   ${CURRENT_DATE}-${CURRENT_MONTH}-${CURRENT_YEAR}",
      "/*/",
      "",
      "Static Function xPrintRel(ccAlias,cPerg)",
      "",
      "\tlocal oReport",
      "\tlocal oSection1",
